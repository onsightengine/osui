{
    "source": {
        "include": "src/",
        "excludePattern": "(node_modules|lib|test)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "destination": "docs"
    },
    "template": {
        "applicationName": "{ suey }",
        "appBar": {
            "items": {
                "repository": {
                    "name": "GitHub",
                    "uri": "https://github.com/salinityengine/suey"
                }
            }
        },
        "repository": "https://github.com/salinityengine/suey",
        "outputSourceFiles": false
    }
}